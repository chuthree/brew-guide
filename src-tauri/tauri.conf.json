{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "Brew Guide",
  "version": "1.5.11-beta.4",
  "identifier": "com.brewguide.desktop",
  "build": {
    "frontendDist": "https://coffee.chu3.top",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": ""
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "Brew Guide",
        "width": 1200,
        "height": 800,
        "minWidth": 375,
        "minHeight": 500,
        "resizable": true,
        "fullscreen": false,
        "center": true,
        "titleBarStyle": "Overlay",
        "hiddenTitle": true,
        "trafficLightPosition": {
          "x": 16,
          "y": 16
        }
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "macOS": {
      "minimumSystemVersion": "10.15",
      "frameworks": [],
      "entitlements": null,
      "signingIdentity": null,
      "providerShortName": null,
      "dmg": {
        "appPosition": {
          "x": 180,
          "y": 170
        },
        "applicationFolderPosition": {
          "x": 480,
          "y": 170
        },
        "windowSize": {
          "width": 660,
          "height": 400
        }
      }
    },
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    },
    "linux": {
      "appimage": {
        "bundleMediaFramework": false
      }
    },
    "shortDescription": "Coffee brewing assistant app",
    "longDescription": "Brew Guide is a comprehensive coffee brewing assistant that helps you perfect your coffee brewing process with timers, notes, and equipment management.",
    "category": "Lifestyle"
  }
}
